{
	"env": {
		"browser": true,
		"es2021": true,
		"mocha": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "windows"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"semi-spacing": "error",
		"comma-dangle": ["error", "always-multiline"]
	}
}
